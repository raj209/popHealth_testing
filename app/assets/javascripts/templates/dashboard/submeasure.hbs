<div class="measure-identifier">
  {{#if isPrimary}}
    {{#if cms_id}}
      <div class="cms-cms">CMS</div>
      <div class="cms-number">{{cms_number}}</div>
      <div class="cms-version">v{{cms_version}}</div>
    {{/if}}
  {{/if}}
</div>
<div{{#if isPrimary}} class="measure-info"{{/if}}>
  <div class="measure-title-id">
    <div class="measure-title" data-measure-sub="{{sub_id}}">
      {{#if isPrimary}}
        {{!-- ADD Measure display Logic --}}
        <a href="#measures/{{id}}{{#if sub_id}}/{{sub_id}}{{/if}}{{#if provider_id}}/providers/{{provider_id}}{{/if}}">{{title}}</a>
        {{#unless short_subtitle}}<span class="glyphicon glyphicon-info-sign icon-popover" data-placement="bottom" data-content="{{description}}" data-trigger="hover focus"></span>{{/unless}}
      {{/if}}
    </div>
    <div class="measure-subtitle">
      {{#if short_subtitle}}
        <a href="#measures/{{id}}/{{sub_id}}{{#if provider_id}}/providers/{{provider_id}}{{/if}}">{{short_subtitle}}</a>
        <span class="glyphicon glyphicon-info-sign icon-popover" data-placement="bottom" data-content="{{description}}" data-trigger="hover focus"></span>
      {{/if}}
    </div>
  </div>
  {{view "ResultsView" model=query id=hqmf_id sub_id=sub_id provider_id=provider_id}}
</div>
<!-- <div class="pull-left {{id}}{{#if sub_id}}{{sub_id}}{{/if}}-loading-measure" hidden="true"><h2>LOADING...</h2></div> -->
<div class="pull-left loader" hidden="true"><h2>LOADING...</h2></div>
